[user]
	name = jkattakkyamsure
	email = jerrin_suresh@apple.com
[core]
	editor = vim
[rerere]
    enabled = 1
[merge]
    conflictstyle = diff3
[color]
    ui = auto
[branch]
    autosetuprebase = always
[branch "master"]
    rebase = true
[push]
    default = tracking
[pull]
    rebase = true
[credential]
    helper = osxkeychain
[alias]
  co = checkout
  ci = commit
  st = status
  br = branch
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  type = cat-file -t
  dump = cat-file -p
  tree = log --graph --pretty=oneline --abbrev-commit --decorate --color --all
  local = log --pretty=oneline @{u}..HEAD
  adc  = add changelog
  adp  = add -p
  adr  = add README
  adv  = add -v
  br  = branch
  bra  = branch -a
  brd  = branch -d
  brdf  = branch -D
  brm  = branch -m
  cf  = config -e --global
  ci  = commit -v
  cis  = commit -v -s
  cia  = commit --amend
  cic  = commit -v -c
  cics  = commit -v -c -s
  cim  = commit -v -m
  cims  = commit -v -m -s
  co  = checkout
  cob  = checkout -b
  cp  = cherry-pick -n
  di  = diff
  difa  = diff HEAD
  difc  = diff --cached
  difcs  = diff --cached --stat
  difs  = diff -b --stat
  dump  = cat-file -p
  hist  = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  la  = "!grep '  = ' ~/.gitconfig | less"
  lg  = log --graph --pretty=format:'%C(cyan)%h%Creset -%C(red)%d%Creset %s %C(green)(%cr)%Creset %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
  llg  = log --graph --pretty=format:'%C(cyan)%h%Creset -%C(red)%d%Creset %s %C(green)(%cr)%Creset %C(bold blue)<%an>%Creset' --abbrev-commit --date=local
  lga  = log --graph --decorate --pretty=oneline --abbrev-commit --all
  lgc  = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
  lgf  = log -p
  lgs  = log --stat
  lgx  = log --graph --pretty=format:'%C(cyan)%h%Creset -%C(red)%d%Creset %s %C(green)(%cr)%Creset %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative --stat
  llog  = log --date=local
  ls  = ls-files
  mrg  = merge --log
  mrgnf  = merge --log --no-ff
  plr  = pull -r
  po  = push origin
  ra  = remote add
  rb  = rebase
  rba  = rebase --abort
  rbc  = rebase --continue
  rbi  = rebase -i
  rl  = remote -v
  rr  = remote rm
  rs  = reset
  rs-hard  = reset --hard
  rt  = remote
  rta  = remote add
  rso  = remote show origin
  rpo  = remote prune origin
  rpon  = remote prune origin --dry-run
  st  = status
  sta  = stash apply
  staged  = diff --cached
  std  = stash drop
  stl  = stash list
  stp  = stash pop
  sts  = stash save
  type  = cat-file -t
  unstage  = reset HEAD
